    "rimraf": "^3.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.19.0",
    "typescript": "^4.7.4",
    "webpack-bundle-analyzer": "^4.7.0"
